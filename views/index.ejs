<html>
    <title>
        messenger
    </title>
    <body>
        <% for (let index = 0; index < messages.length; index++) { %>
            <ul>
                <li>User: <%= messages[index].user %></li>
                <li>Text: <%= messages[index].text %></li>
                <li>Date: <%= messages[index].added %></li>
            </ul>
        <% } %> 
    </body>
</html>